{
  "manifest_version": 3,
  "name": "Cyber Awareness Chatbot",
  "version": "1.0",
  "description": "AI chatbot to verify links and report suspicious websites",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}

  